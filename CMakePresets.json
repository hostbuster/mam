{
  "version": 3,
  "configurePresets": [
    {
      "name": "default",
      "generator": "Ninja",
      "binaryDir": "build",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "RelWithDebInfo",
        "MAM_WARNINGS_AS_ERRORS": "ON",
        "MAM_USE_JSON_SCHEMA": "ON"
      }
    },
    {
      "name": "asan",
      "inherits": ["default"],
      "cacheVariables": {
        "MAM_ENABLE_ASAN": "ON"
      }
    },
    {
      "name": "ubsan",
      "inherits": ["default"],
      "cacheVariables": {
        "MAM_ENABLE_UBSAN": "ON"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "default",
      "configurePreset": "default"
    }
  ]
}


