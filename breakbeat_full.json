{
  "version": 1,
  "sampleRate": 48000,
  "channels": 2,

  "nodes": [
    {
      "id": "kick1",
      "type": "kick",
      "params": { "f0": 120, "fend": 35, "pitchDecayMs": 55, "ampDecayMs": 190, "gain": 0.9, "click": 0.1, "bpm": 0, "loop": false }
    },
    {
      "id": "snr1",
      "type": "clap",
      "params": { "ampDecayMs": 180, "gain": 0.85, "bpm": 0, "loop": false }
    },
    {
      "id": "hat1",
      "type": "clap",
      "params": { "ampDecayMs": 40, "gain": 0.55, "bpm": 0, "loop": false }
    },
    {
      "id": "hat2",
      "type": "clap",
      "params": { "ampDecayMs": 25, "gain": 0.45, "bpm": 0, "loop": false }
    },
    {
      "id": "transport1",
      "type": "transport",
      "params": {
        "bpm": 130,
        "lengthBars": 16,
        "resolution": 16,
        "swingPercent": 8,
        "patterns": [
          { "nodeId": "kick1", "steps": "x...x...x..x...." },
          { "nodeId": "snr1",  "steps": "....x.......x..." },
          { "nodeId": "hat1",  "steps": "x.x.x.x.x.x.x.x." }
        ]
      }
    }
  ],

  "mixer": {
    "masterPercent": 90,
    "softClip": true,
    "inputs": [
      { "id": "kick1", "gainPercent": 85 },
      { "id": "snr1",  "gainPercent": 75 },
      { "id": "hat1",  "gainPercent": 75 },
      { "id": "hat2",  "gainPercent": 65 }
    ]
  },

  "transport": {
    "bpm": 130,
    "lengthBars": 16,
    "resolution": 16,
    "swingPercent": 8,
    "patterns": [
      { "nodeId": "kick1", "steps": "x...x...x..x...." },
      { "nodeId": "snr1",  "steps": "....x.......x..." },
      { "nodeId": "hat1",  "steps": "x.x.x.x.x.x.x.x." }
    ]
  },

  "commands": [
    { "sampleTime": 0, "nodeId": "kick1", "type": "SetParam", "param": "GAIN", "value": 0.95 },
    { "sampleTime": 0, "nodeId": "snr1",  "type": "SetParam", "param": "GAIN", "value": 0.85 },
    { "sampleTime": 0, "nodeId": "hat1",  "type": "SetParam", "param": "GAIN", "value": 0.65 }
