{
  "version": 1,
  "randomSeed": 1337,
  "sampleRate": 48000,
  "channels": 2,

  "nodes": [
    {
      "id": "kick1",
      "type": "kick",
      "params": { "f0": 120, "fend": 35, "pitchDecayMs": 55, "ampDecayMs": 190, "gain": 0.9, "click": 0.1, "bpm": 0, "loop": false }
    },
    {
      "id": "snr1",
      "type": "clap",
      "params": { "ampDecayMs": 180, "gain": 0.85, "bpm": 0, "loop": false }
    },
    { "id": "delay1", "type": "delay", "params": { "delayMs": 350, "feedback": 0.35, "mix": 0.25 } },
    { "id": "delay2", "type": "delay", "params": { "delayMs": 190, "feedback": 0.25, "mix": 0.18 } },
    { "id": "meter1", "type": "meter", "params": { "target": "delay1" } }
  ],

  "mixer": {
    "masterPercent": 90,
    "softClip": true,
    "inputs": [
      { "id": "kick1", "gainPercent": 85 },
      { "id": "snr1",  "gainPercent": 75 },
      { "id": "delay1",  "gainPercent": 100 }
    ]
  },

  "transport": {
    "bpm": 126,
    "lengthBars": 8,
    "resolution": 16,
    "patterns": [
      { "nodeId": "kick1", "steps": "x...x...x...x..." },
      { "nodeId": "snr1",  "steps": "....x.......x..." }
    ]
  },

  "connections": [
    { "from": "kick1", "to": "delay1" },
    { "from": "snr1",  "to": "delay1" },
    { "from": "delay1", "to": "delay2" }
  ],

  "commands": []
}
