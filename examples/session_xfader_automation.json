{
  "kind": "session",
  "version": 1,
  "sampleRate": 48000,
  "channels": 2,
  "racks": [
    { "id": "rack1", "path": "examples/acid303.json", "gain": 1.0, "loopSeconds": 30, "muted": false, "solo": false },
    { "id": "rack2", "path": "examples/mamic_song.json", "gain": 1.0, "bars": 32, "loopCount": 1, "muted": false, "solo": false }
  ],
  "xfaders": [
    { "id": "main", "racks": ["rack1","rack2"], "law": "equal_power", "smoothingMs": 50.0 }
  ],
  "commands": [
    { "timeSec": 0.0,  "nodeId": "xfader:main:x", "type": "SetParam", "value": 0.5, "rampMs": 200 },
    { "timeSec": 3.0,  "nodeId": "xfader:main:x", "type": "SetParam", "value": 1.0, "rampMs": 800 },
    { "timeSec": 6.0,  "nodeId": "xfader:main:x", "type": "SetParam", "value": 0.25, "rampMs": 400 }
  ]
}


